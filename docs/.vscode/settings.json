{
    "latex-workshop.latex.outDir": "build",
    "latex-workshop.latex.recipes":[
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        },
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "-pdflatex=lualatex",
                "%DOC%"
            ],
            "env": {}
        },
    ],
    "cSpell.language": "en-GB",
    "cSpell.words": [
        "Sandmark",
        "aslr",
        "bytecode",
        "caml",
        "krishnaswami",
        "neel",
        "o",
        "speedup",
        "speedups",
        "toplevel"
    ]
}