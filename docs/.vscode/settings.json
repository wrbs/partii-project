{
  "latex-workshop.latex.outDir": "build",
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk",
      "tools": ["latexmk"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-shell-escape",
        "-outdir=%OUTDIR%",
        "-pdflatex=lualatex",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.latexindent.args": [
    "-m",
    "-c",
    "%DIR%/",
    "%TMPFILE%",
    "-y=defaultIndent:'%INDENT%',modifyLineBreaks:condenseMultipleBlankLinesInto:1;textWrapOptions:columns:100"
  ],
  "cSpell.language": "en-GB",
  "cSpell.words": [
    "Sandmark",
    "aslr",
    "bytecode",
    "caml",
    "krishnaswami",
    "neel",
    "o",
    "speedup",
    "speedups",
    "toplevel"
  ]
}
